{{#data}}
<h2>{{name}}</h2>
<div id="couponresult">
  <form id="buyCoupon">
    <p>
      <label>Amount: </label>
      <input name="amounttobuy" type="text" class="amounttobuy" size="6">
      <input type="submit" value="Buy" />
    </p>
  </form>
<p>ID: {{ id }}</p>
<p>Description: {{ description }}</p>
<p><img src="{{ logo_url }}"/></p>
<p>Type: {{ coupontype }}</p>
</div>
{{/data}}